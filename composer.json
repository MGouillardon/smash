{
    "name": "acs/smash",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Database\\": "database/"
        },
        "files": [
            "config.php",
            "helpers.php"
        ]
    },
    "require-dev": {
        "symfony/var-dumper": "^6.2",
        "nunomaduro/phpinsights": "^2.6"
    },
    "require": {
        "robmorgan/phinx": "^0.13.3",
        "symfony/http-client": "^6.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
